# variable re-definition, dynamic typing
say "Testing variable re-definition:"
let y   # y is nil
let x = 7 * 5
say "x = " + to_str(x)

x = "x is now a string" # redefine x as a string
say x

say "y = " + to_str(y)
y = "y is now a string" # redefine y as a string
say y

let z = 10 - 1
say "Seven ate " + to_str(z)  # "Seven ate 9.000"