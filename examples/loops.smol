say "fizzbuzz-level achieved\n"
let i = 1
say "[" + to_str(i) + "] "
while (i <= 100) {
    if (i % 15 == 0)
        say "fizzbuzz "
    elif (i % 3 == 0)
        say "fizz "
    elif (i % 5 == 0)
        say "buzz "
    else
        say to_str(i) + " "

    if (i % 5 == 0)
        say "\n[" + to_str(i + 1) + "] "
    i = i + 1
}

i = 9.06
say "checking precision for doubles: " + to_str(i) + "\n"

for (let j = 0; j < 5; j = j + 1) {
    say to_str(j) + "\n"
}
