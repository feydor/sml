say "fizzbuzz-level achieved:"
let i = 1
while (i <= 100) {
    if (i % 15 == 0)
        say "fizzbuzz"
    elif (i % 3 == 0)
        say "fizz"
    elif (i % 5 == 0)
        say "buzz"
    else
        say to_str(i)
    i++
}

i = 9.06
say "checking precision for doubles: " + to_str(i)

say "for 0...4"
for (let j = 0; j < 5; j++)
    say to_str(j)
